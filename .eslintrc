{
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "no-console": 0,
    "import": 0,
    "no-unused-vars": 1,
    "arrow-body-style": [2, "as-needed"],
    "no-param-reassign": 0
  },
  "plugins": []
}
